{
  "name": "manga-scraper-api",
  "version": "1.0.0",
  "description": "manga scraper api",
  "main": "dist/mango-scraper-api/src/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "dotenv -e .env.development -- npx nodemon src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "prisma generate",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev --name=migration"
  },
  "prisma": {
    "schema": "./src/config/prisma/schema.prisma"
  },
  "author": "Pierrot",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.16",
    "cpx": "^1.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@types/body-parser": "^1.19.5",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "express": "^4.18.2",
    "prisma": "^5.11.0"
  }
}
